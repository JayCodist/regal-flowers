{
  "hosting": {
    "site": "regalflowers",
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "destination": "/404.html",
        "dynamicLinks": true
      }
    ],
    "redirects": [       
      {
        "source": "/contact-us",
        "destination": "/#contactSection",
        "type": 301
      },      
      {
        "source": "/faqs-how-to-order-and-delivery-information",
        "destination": "/faq",
        "type": 301
      },       
      {
        "source": "/delivery-information",
        "destination": "/faq",
        "type": 301
      },      
