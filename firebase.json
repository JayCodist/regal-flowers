        "type": 301
      },
      {
        "source": "/product-category/gift-items-perfumes-cakes-chocolate-wine-and-teddy-bears/chocolate-and-biscuits",
        "destination": "/product-category/chocolate-and-biscuits",
        "type": 301
      }, 
      {
        "source": "/product-category/gift-items-perfumes-cakes-chocolate-wine-and-teddy-bears/cakes-and-cupcakes",
        "destination": "/product-category/cakes-and-cupcakes",
        "type": 301
      }, 
