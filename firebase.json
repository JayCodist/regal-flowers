{
  "hosting": {
    "site": "regalflowers",
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "destination": "/404.html",
        "dynamicLinks": true
      }
    ],
    "redirects": [
      {
        "source": "/product/vip-ferrero-rocher-roses-bouquet-bouquet-of-roses-and-ferrero-rocher-chocolatui",
        "destination": "/product/vip-ferrero-rocher-roses-bouquet-bouquet-of-roses-and-ferrero-rocher-chocolates",
        "type": 301
      },
      {
        "source": "/product-category/funeral-flowers",
        "destination": "/product-category/funeral-amp-condolence",
        "type": 301
      },
      {
        "source": "/product-category/love-romance-birthday-anniversary-just-to-say-flowerss",
        "destination": "/product-category/birthday-anniversary-love-romance-mothers-day-and-valentine-flowers",
        "type": 301
      }
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|png|svg|js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ],
    "trailingSlash": false
  }
}
